{
  "name": "fvttdev",
  "description": "Provides a zero config build tool for Foundry VTT modules / systems based on Rollup and Oclif",
  "version": "0.0.0",
  "author": "typhonrt",
  "bin": {
    "fvttdev": "./bin/run"
  },
  "bugs": "https://github.com/typhonjs-fvtt/fvttdev/issues",
  "dependencies": {
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/plugin-help": "^3.2.1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.26.0",
    "@oclif/test": "^1.2.8",
    "chai": "^4.2.0",
    "eslint": "^5.16.0",
    "eslint-config-oclif": "^3.1.0",
    "globby": "^10.0.2",
    "mocha": "^5.2.0",
    "nyc": "^14.1.1"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/src"
  ],
  "homepage": "https://github.com/typhonjs-fvtt/fvttdev",
  "keywords": [
    "oclif"
  ],
  "license": "MPL2.0",
  "main": "src/index.js",
  "oclif": {
    "commands": "./src/commands",
    "bin": "fvttdev",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "typhonjs-fvtt/fvttdev",
  "scripts": {
    "run-hello": "fvttdev build",
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint .",
    "prepack": "oclif-dev manifest && oclif-dev readme && oclif-dev manifest && oclif-dev readme",
    "test": "nyc mocha --forbid-only \"test/**/*.test.js\"",
    "version": "oclif-dev readme && git add README.md"
  }
}
