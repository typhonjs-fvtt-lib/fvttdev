{
  "name": "fvttdev",
  "description": "Provides a zero config CLI build tool for Foundry VTT modules / systems based on Rollup and Oclif",
  "version": "0.0.0",
  "author": "typhonrt",
  "bin": {
    "fvttdev": "./bin/run"
  },
  "private": true,
  "bugs": "https://github.com/typhonjs-fvtt/fvttdev/issues",
  "dependencies": {
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/plugin-commands": "^1.3.0",
    "@oclif/plugin-help": "^3.2.1",
    "@oclif/plugin-not-found": "^1.2.4",
    "@oclif/plugin-plugins": "^1.9.4",
    "@oclif/plugin-which": "^1.0.3",
    "backbone-esnext-events": "^0.3.0",
    "dotenv": "^8.0.0",
    "fs-jetpack": "^4.0.0",
    "typhonjs-color-logger": "^0.0.13",
    "typhonjs-package-util": "^0.0.3",
    "typhonjs-plugin-manager": "^0.2.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.26.0",
    "@oclif/test": "^1.2.8",
    "chai": "^4.2.0",
    "eslint": "^5.16.0",
    "globby": "^10.0.2",
    "mocha": "^5.2.0",
    "nyc": "^14.1.1",
    "rollup": "^2.0.0",
    "typhonjs-config-eslint": "^0.6.0"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/src"
  ],
  "homepage": "https://github.com/typhonjs-fvtt/fvttdev",
  "keywords": [
    "oclif"
  ],
  "license": "MPL-2.0",
  "main": "src/index.js",
  "oclif": {
    "commands": "./src/commands",
    "hooks": {
      "init": [
        "./src/hooks/init/init",
        "./src/hooks/init/bundle"
      ]
    },
    "bin": "fvttdev",
    "plugins": [
      "@oclif/plugin-commands",
      "@oclif/plugin-help",
      "@oclif/plugin-not-found",
      "@oclif/plugin-plugins",
      "@oclif/plugin-which"
    ]
  },
  "repository": "typhonjs-fvtt/fvttdev",
  "scripts": {
    "run-build": "fvttdev bundle",
    "run-build-env": "fvttdev bundle -e test",
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint .",
    "prepack": "oclif-dev manifest && oclif-dev readme && oclif-dev manifest && oclif-dev readme",
    "test": "nyc mocha --forbid-only \"test/**/*.test.js\"",
    "version": "oclif-dev readme && git add README.md"
  }
}
